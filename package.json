{
  "name": "next-graphql",
  "version": "1.0.0",
  "keywords": [
    "nextjs",
    "nexus",
    "prisma",
    "pothos",
    "urql"
  ],
  "author": "SoraKumo",
  "license": "MIT",
  "description": "",
  "scripts": {
    "dev:next": "next",
    "dev:docker": "docker compose -p next-graphql -f docker/docker-compose.yml up -d",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --fix ./src",
    "lint:fix": "eslint --fix ./src",
    "prisma:reset": "next-exec prisma migrate reset -f",
    "prisma:migrate": "prisma format && prisma migrate dev",
    "prisma:generate": "prisma generate",
    "graphql:codegen": "graphql-codegen --config codegen/codegen.ts"
  },
  "dependencies": {
    "@apollo/explorer": "^3.6.0",
    "@pothos/core": "^3.38.0",
    "@pothos/plugin-prisma": "^3.61.0",
    "@pothos/plugin-prisma-utils": "^0.12.0",
    "@prisma/client": "^5.4.2",
    "@react-libraries/next-exchange-ssr": "^1.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^4.0.5",
    "next": "^13.5.4",
    "pothos-prisma-generator": "^1.0.1",
    "pothos-query-generator": "^0.0.2",
    "pothos-schema-exporter": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "urql": "^4.0.5",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@types/cookie": "^0.5.2",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.16",
    "eslint": "8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "postcss": "^8.4.31",
    "prisma": "^5.4.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "main": "index.js",
  "repository": "https://github.com/SoraKumo001/next-graphql"
}
